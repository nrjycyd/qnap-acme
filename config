# acme 邮箱注册
EMAIL=abc123@163.com

# acme 域名设置
DOMAIN=abc123.com

# CA 机构选择
CA="zerossl"  # 可选择 "letsencrypt"、"buypass" 或 "zerossl"

# DNS类型，根据域名服务商而定
DNS="dns_ali"  # 可选择 "dns_ali"、"dns_cf" 或 "dns_dp"

# dns_ali 设置
Ali_Key="ABC*********123"
Ali_Secret="ABC*******************123"

# dns_cf 设置
CF_Key="abc***********123"
CF_Email="alice@example.com"

# dns_dp 设置
DP_Id="<id>"
DP_Key="<key>"



